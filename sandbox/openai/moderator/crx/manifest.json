{
  "manifest_version": 3,
  "name": "Moderator Extension",
  "version": "1.0",
  "description": "Moderator Chrome Extension",
  "permissions": ["storage", "contextMenus", "sidePanel"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}
